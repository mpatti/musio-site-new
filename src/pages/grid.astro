<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <title>Instrument Grid - Screenshot Utility</title>
  <style>
    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }
    body {
      background: #0a0a0a;
    }
    .grid {
      display: grid;
      grid-template-columns: repeat(10, 200px);
      gap: 8px;
      padding: 8px;
    }
    .grid img {
      width: 200px;
      height: 200px;
      object-fit: cover;
      border-radius: 8px;
    }
    .grid img.broken {
      background: #ff0000;
    }
  </style>
</head>
<body>
  <div class="grid">
    {instrumentImages.map((src, i) => (
      <img 
        src={src} 
        alt={`Instrument ${i + 1}`}
        onerror="this.classList.add('broken')"
      />
    ))}
  </div>
</body>
</html>
